{
  "@context": {
    "edc": "https://w3id.org/edc/v0.0.1/ns/",
    "odrl": "http://www.w3.org/ns/odrl.jsonld"
  },
  "@type": "PolicyDefinitionRequest",
  "@id": "ID-policy",
  "policy": {
    "@type": "odrl:Set",
    "permission": [
      {
        "action": {
          "@id": "odrl:use"
        },
        "target": "test-asset",
        "constraint": [
          {
          "@type": "odrl:Constraint",
          "leftOperand": "ID",
          "operator": {
            "@id": "odrl:IS_ANY_OF"
          },
          "rightOperand": ["manufacturer", "supplier", "recycler"]
          }
        ]
      }
    ],
    "prohibition": [],
    "obligation": []
  }
}